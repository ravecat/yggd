{
  "name": "nextjs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/nextjs/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev",
        "cwd": "apps/nextjs"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/nextjs"
      },
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/tsconfig.base.json"
      ]
    }
  },
  "tags": []
}
